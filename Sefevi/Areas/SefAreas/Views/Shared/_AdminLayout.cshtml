                            </li>
                            <li class="nav-item ">
                                <a class="nav-link " href="@Url.Action("Index","AddProduct")"><i class="fa fa-fw fa-user-circle"></i>Product</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link " href="@Url.Action("Index","AddTopSlider")"><i class="fa fa-fw fa-user-circle"></i>Slider Top</a>

                            </li>
                            <li class="nav-item ">
                                <a class="nav-link " href="@Url.Action("Index","AboutUs")"><i class="fa fa-fw fa-user-circle"></i>About Us</a>
