    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Blank Page</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Areas/SefAreas/assets/vendor/bootstrap/css/bootstrap.min.css">
    <link href="~/Areas/SefAreas/assets/vendor/fonts/circular-std/style.css" rel="stylesheet">
